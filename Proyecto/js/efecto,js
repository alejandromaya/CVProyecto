    function qiehuan(){
        document.getElementById("img").src=imgs[index];
        index++;
        if(index > 3){
                         index = 0; // Solo hay cuatro im치genes, restablezca a 0 cuando sea 3
        }
    }
  setInterval("qiehuan()", 1800); // vuelva a ejecutar la funci칩n cada 3 segundos qiehuan ()